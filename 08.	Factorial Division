using System;

namespace Factorial_Division
{
	class Program
	{

		static long Factorial (long number)
		{
			long factorial = 1;

			for (int i = 1; i <= number; i++)
			{
				factorial *= i;
			}

			return factorial;
		}

		static void Main(string[] args)
		{
			double factorialOne = 0;
			double factorialTwo = 0;

			if (true)
			{
				long number = int.Parse(Console.ReadLine());

				factorialOne = Factorial(number);
			}

			if (true)
			{
				long number = int.Parse(Console.ReadLine());

				factorialTwo = Factorial(number);
			}

			Console.WriteLine($"{factorialOne / factorialTwo:f2}");
		}
	}
}
